<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >  

<hibernate-mapping package="com.lzz.order.pojo">
	<class name="LzzUser" table="lzzuser">
		<id name="id" column="id" type="string"></id>

				<property name="openId" type="string">
					<column name="openId" length="255"/>
				</property>

				<property name="unionId" type="string">
					<column name="unionId" length="255"/>
				</property>

				<property name="nick_name" type="string">
					<column name="nick_name" length="255"/>
				</property>

				<property name="avatarUrl" type="text">
					<column name="avatarUrl" length="1000"/>
				</property>

				<property name="sex" type="string">
					<column name="sex" length="10"/>
				</property>

				<property name="phone" type="string">
					<column name="phone" length="20"/>
				</property>

				<property name="pwd" type="string">
					<column name="pwd" length="255"/>
				</property>

				<property name="level" type="string">
					<column name="level" length="255"/>
				</property>

				<property name="isManager" type="string">
					<column name="isManager" length="1"/>
				</property>

				<property name="total_integration" type="string">
					<column name="total_integration" length="255"/>
				</property>

				<property name="cur_integration" type="string">
					<column name="cur_integration" length="255"/>
				</property>

				<property name="validate_flag" type="string">
					<column name="validate_flag" length="1"/>
				</property>

				<property name="createTime" type="string">
					<column name="createTime" length="20"/>
				</property>

				<property name="modifyTime" type="string">
					<column name="modifyTime" length="20"/>
				</property>

				<property name="def1" type="text">
					<column name="def1" length="500"/>
				</property>

				<property name="def2" type="text">
					<column name="def2" length="500"/>
				</property>

				<property name="def3" type="text">
					<column name="def3" length="500"/>
				</property>

				<property name="def4" type="text">
					<column name="def4" length="500"/>
				</property>

				<property name="def5" type="text">
					<column name="def5" length="500"/>
				</property>

				<property name="def6" type="text">
					<column name="def6" length="500"/>
				</property>

				<property name="def7" type="text">
					<column name="def7" length="500"/>
				</property>

				<property name="def8" type="text">
					<column name="def8" length="500"/>
				</property>

				<property name="def9" type="text">
					<column name="def9" length="500"/>
				</property>

				<property name="def10" type="text">
					<column name="def10" length="500"/>
				</property>

				<property name="dr" type="string">
					<column name="dr" length="1"/>
				</property>

	</class>
</hibernate-mapping>
