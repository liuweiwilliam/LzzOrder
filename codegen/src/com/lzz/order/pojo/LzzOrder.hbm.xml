<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >  

<hibernate-mapping package="com.lzz.order.pojo">
	<class name="LzzOrder" table="lzzorder">
		<id name="id" column="id" type="string"></id>

				<property name="userId" type="string">
					<column name="userId" length="255"/>
				</property>

				<property name="orderNum" type="string">
					<column name="orderNum" length="255"/>
				</property>

				<property name="payTime" type="string">
					<column name="payTime" length="20"/>
				</property>

				<property name="address" type="text">
					<column name="address" length="1000"/>
				</property>

				<property name="expressCompany" type="string">
					<column name="expressCompany" length="255"/>
				</property>

				<property name="expressNum" type="string">
					<column name="expressNum" length="255"/>
				</property>

				<property name="expressFee" type="string">
					<column name="expressFee" length="255"/>
				</property>

				<property name="status" type="string">
					<column name="status" length="255"/>
				</property>

				<property name="evaluateTime" type="string">
					<column name="evaluateTime" length="20"/>
				</property>

				<property name="finishTime" type="string">
					<column name="finishTime" length="20"/>
				</property>

				<property name="couponFlag" type="string">
					<column name="couponFlag" length="1"/>
				</property>

				<property name="couponId" type="string">
					<column name="couponId" length="255"/>
				</property>

				<property name="couponPrice" type="string">
					<column name="couponPrice" length="255"/>
				</property>

				<property name="fromWhere" type="string">
					<column name="fromWhere" length="255"/>
				</property>

				<property name="comment" type="text">
					<column name="comment" length="500"/>
				</property>

				<property name="createTime" type="string">
					<column name="createTime" length="20"/>
				</property>

				<property name="modifyTime" type="string">
					<column name="modifyTime" length="20"/>
				</property>

				<property name="def1" type="text">
					<column name="def1" length="500"/>
				</property>

				<property name="def2" type="text">
					<column name="def2" length="500"/>
				</property>

				<property name="def3" type="text">
					<column name="def3" length="500"/>
				</property>

				<property name="def4" type="text">
					<column name="def4" length="500"/>
				</property>

				<property name="def5" type="text">
					<column name="def5" length="500"/>
				</property>

				<property name="def6" type="text">
					<column name="def6" length="500"/>
				</property>

				<property name="def7" type="text">
					<column name="def7" length="500"/>
				</property>

				<property name="def8" type="text">
					<column name="def8" length="500"/>
				</property>

				<property name="def9" type="text">
					<column name="def9" length="500"/>
				</property>

				<property name="def10" type="text">
					<column name="def10" length="500"/>
				</property>

				<property name="dr" type="string">
					<column name="dr" length="1"/>
				</property>

	</class>
</hibernate-mapping>
